{
  "name": "neuenable",
  "version": "0.5.2",
  "description": "create neumorphic style elements",
  "main": "dist/js/neuenable.js",
  "directories": {
    "doc": "docs",
    "dist": "dist"
  },
  "scripts": {
    "test": "sass tests/sass_tests.scss",
    "sass": "sass -w src/scss:dist/css",
    "sass:min": "sass -w src/scss/neuenable.scss:dist/css/neuenable.min.css --style compressed",
    "sass:build": "sass src/scss:dist/css",
    "sass:lint": "npx stylelint '**/*.scss'",
    "check-content": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rain-kun/neuenable.git"
  },
  "keywords": [
    "neumorphism",
    "neumorphic",
    "ui-components"
  ],
  "author": "Ashutosh Yadav",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rain-kun/neuenable/issues"
  },
  "homepage": "https://github.com/rain-kun/neuenable#readme",
  "dependencies": {
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.21.0"
  },
  "devDependencies": {
    "sass": "^1.40.0",
    "sass-true": "^6.0.1",
    "stylelint": "^13.13.1"
  }
}
